@model IEnumerable<_1911065331_BuiTanPhat_BigSchools.Models.Course>
@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <ul class="course crs">
        @foreach (var course in Model)
        {
            <li>
                <div class="date">
                    <div class="month">
                        @course.DateTime.ToString("MM")
                    </div>
                    <div class="day">
                        @course.DateTime.ToString("dd")
                    </div>
                </div>
                <div class="details">
                    <span class="lecturer">
                        @course.Lecturer.Name
                    </span>
                    <span class="category">
                        <br />
                        @course.Category.Name
                    </span>

                </div>
            </li>
        }
    </ul>

</div>
